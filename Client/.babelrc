{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current",
          "esmodules": true
        },
        "bugfixes": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [

    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
